{
    "extends": "tslint:recommended",
    "defaultSeverity": "error",
    "plugins": [
        "react-hooks"
    ],
    "jsRules": {
        "variable-name": false,
        "react-hooks/rules-of-hooks": "error",
        "react-hooks/exhaustive-deps": "warn"
    },
    "rules": {
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "no-console": {
            "severity": "warning"
        },
        "variable-name": {
            "options": "allow-leading-underscore"
        }
    }
}
